{
  "name": "python-package-inspector",
  "displayName": "Python Package Inspector",
  "description": "Check whether python packages suggested by AI are well maintained and popular ... or if they even exist.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "python-package-inspector.helloWorld",
        "title": "Hello World"
      }
    ],
    "configuration": {
      "title": "Python Package Inspector",
      "properties": {
        "pythonPackageInspector.enable": {
          "type": "boolean",
          "default": true,
          "description": "Enable/disable this extension"
        },
        "pythonPackageInspector.cacheTimeout": {
          "type": "number",
          "default": 60,
          "description": "Set the timeout (in minutes) for cached package information"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "typescript": "5.6"
  },
  "dependencies": {
    "undici": "^6.19.8"
  }
}
